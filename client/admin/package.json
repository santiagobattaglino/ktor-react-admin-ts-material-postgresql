{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "@types/classnames": "^2.3.1",
    "@types/concurrently": "^6.0.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.43",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/testing-library__jest-dom": "^5.9.5",
    "classnames": "^2.3.1",
    "concurrently": "^6.0.2",
    "prop-types": "^15.7.2",
    "ra-data-simple-rest": "^3.13.4",
    "react": "^17.0.2",
    "react-admin": "^3.15.0",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.2.4",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "dev": "concurrently \"npm run server-dev\" \"npm run client-dev\"",
    "server-dev": "cd ../../ && ./gradlew run",
    "client-dev": "npm run start",
    "test-server": "cd ../../ && ./gradlew test",
    "prod": "concurrently \"npm run server-prod\" \"npm run client-dev\"",
    "server-prod": "java -jar ../../build/libs/jdbcat-ktor-backend.jar",
    "client-prod": "npm run build",
    "start": "react-scripts start",
    "build": "react-scripts build && rm -R ../../static/ && mkdir ../../static/ && mv -v build/* ../../static/",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy-heroku": "npm run build && git push heroku develop:main && heroku ps:scale web=1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/query-string": "^6.3.0",
    "@types/react-router-dom": "^5.1.7"
  }
}
